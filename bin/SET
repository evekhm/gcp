#!/usr/bin/env bash
##################################################################################
#                        GCP and  Deployment SETTINGS
##################################################################################
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

if [ -z $REGION ]; then
  export REGION='us-west1'
fi

if [ -z $ZONE ]; then
  export ZONE='us-west1-a'
fi
export CONFIG='demo'

source "$DIR/SET_cgm"
echo "Done ${BASH_SOURCE[0]}"




